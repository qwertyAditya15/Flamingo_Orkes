{
	"name": "cf-codeploy-api",
	"version": "0.0.1",
	"description": "API for Codeploy app.",
	"main": "build/index.js",
	"repository": "https://github.com/Ankan002/cf-codeploy-api.git",
	"author": "Runtime Terror (Clan Flare)",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start:dev": "node --env-file=.env --import=extensionless/register build/index.js",
		"start": "node --import=extensionless/register build/src/index.js",
		"dev": "nodemon",
		"build": "swc src -d build",
		"test": "jest --passWithNoTests",
		"test-dev": "jest --passWithNoTests --setupFiles dotenv/config",
		"prepare": "npx husky install",
		"lint": "eslint src/**/*.ts",
		"pre-commit": "yarn lint && yarn test-dev && yarn build",
		"post-merge": "yarn install",
		"commit-msg": "npx --no-install commitlint --edit"
	},
	"dependencies": {
		"@io-orkes/conductor-javascript": "^2.0.0",
		"bcrypt": "^5.1.1",
		"cloudinary": "^1.41.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.3",
		"extensionless": "^1.9.6",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.1.0",
		"morgan": "^1.10.0",
		"winston": "^3.11.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.4",
		"@commitlint/config-conventional": "^18.4.4",
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.104",
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/express-fileupload": "^1.4.4",
		"@types/jest": "^29.5.11",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.5",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.4",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
